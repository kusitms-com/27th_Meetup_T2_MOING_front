<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".login.SplashActivity"
    android:background="#FF5B44">


    <ImageView
        android:id="@+id/logoImg"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginHorizontal="136dp"
        android:layout_marginTop="276dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_img" />

    <ImageView
        android:layout_width="104.91dp"
        android:layout_height="16.77dp"
        android:layout_marginHorizontal="128dp"
        android:layout_marginTop="322dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.473"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logoImg"
        app:srcCompat="@drawable/logo_text_img" />

</androidx.constraintlayout.widget.ConstraintLayout>